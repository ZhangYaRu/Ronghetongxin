package com.zhbd.beidoucommunication.domain;

import java.util.ArrayList;

/**
 * Created by zhangyaru on 2017/9/18.
 */

public class BeidouMachineState {
    // 硬件状态   0 正常,1 异常
    private byte hardwareState;
    // 10个波束信号强度
    // 00: <-158dBW
    // 01: -156~-157dBW
    // 02: -154~-155dBW
    // 03: -152~-153dBW
    // 04: >-152dBW
    private byte[] signalStrength;
    // ic卡数量
    private byte icCardCount;
    // ic卡信息
    private ArrayList<IcInfo> icInfos;

    public static class IcInfo {
        // ic卡编号
        private int num;
        // ic卡号码
        private int icNumber;
        // 入站状态
        private byte enterState;
        // 服务频度
        private byte pinDu;
        // 通信等级
        private byte grade;

        public int getNum() {
            return num;
        }

        public void setNum(int num) {
            this.num = num;
        }

        public int getIcNumber() {
            return icNumber;
        }

        public void setIcNumber(int icNumber) {
            this.icNumber = icNumber;
        }

        public byte getEnterState() {
            return enterState;
        }

        public void setEnterState(byte enterState) {
            this.enterState = enterState;
        }

        public byte getPinDu() {
            return pinDu;
        }

        public void setPinDu(byte pinDu) {
            this.pinDu = pinDu;
        }

        public byte getGrade() {
            return grade;
        }

        public void setGrade(byte grade) {
            this.grade = grade;
        }
    }

    public byte getHardwareState() {
        return hardwareState;
    }

    public void setHardwareState(byte hardwareState) {
        this.hardwareState = hardwareState;
    }

    public byte[] getSignalStrength() {
        return signalStrength;
    }

    public void setSignalStrength(byte[] signalStrength) {
        this.signalStrength = signalStrength;
    }

    public byte getIcCardCount() {
        return icCardCount;
    }

    public void setIcCardCount(byte icCardCount) {
        this.icCardCount = icCardCount;
    }

    public ArrayList<IcInfo> getIcInfos() {
        return icInfos;
    }

    public void setIcInfos(ArrayList<IcInfo> icInfos) {
        this.icInfos = icInfos;
    }
}
